{
  "Logging": {
    "LogLevel": {
      "Default": "debug",
      "Microsoft": "debug",
      "Microsoft.Hosting.Lifetime": "debug"
    }
  },
  "SearchServiceName": "",
  "SearchIndexName": "",
  "SearchServiceAdminApiKey": "",
  "SearchServiceQueryApiKey": "",
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TripleDerby": "Server=db;Database=master;User=sa;Password=Pa55w0rd2019;"
